class Solution {
    public int[] solution(int[] A, int K) {
        // write your code in Java SE 8
        for(int i = 0; i < K; i++){
            int temp = A[A.length - 1];
            for(int j = A.length - 2; j >= 0; j--){
                A[j + 1] = A[j];
            }
            A[0] = temp;
        }
        return A;
    }
}
