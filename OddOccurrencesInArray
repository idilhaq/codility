class Solution {
    public int solution(int[] A) {
        // write your code in Java SE 8
        Arrays.sort(A);
        int result = 0;
        for(int i = 0; i < A.length; i=i+2){
            if(A[i] != A[i+1]){
                return A[i];
            }
        }
        return result;
    }
}
